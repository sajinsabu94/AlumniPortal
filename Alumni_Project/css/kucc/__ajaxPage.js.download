function addtolist(cb){
			var select_list_selected_index = cb.selectedIndex;
			var text = cb.options[select_list_selected_index].value;
			var flg=1;
			var value = cb.value;
			var myList = document.getElementById("lst");
			//if(text==0)
			if(myList.options[0].value==0)
			myList.options[0]=null;
			//alert("MM"+myList.length+" " +text);
			if(cb.options[select_list_selected_index].value!=0)
			{
				for ( var x=0; x<myList.length; x++ ) 
				{
				if(value==myList.options[x].value)
				{
					flg=0;
					break;
				}
				}
				if(flg==1)
				{
				myList.size = myList.size + 1;
				myList.options[myList.length] = new Option(text, value);
				}
				if ( myList.options.length>0 ) {
				var colcod="";
				for ( var x=0; x<myList.length; x++ ) {
				if(colcod!='')
				colcod=colcod+','+myList.options[x].value;
				else
				colcod=colcod+myList.options[x].value;
				//alert("LL"+colcod);
			}
			}
			}
			//alert("LL"+colcod);
		XmlReg = JSfncGetXMLHttpRequest();
		if(!XmlReg){
			alert('Ajax Not Supported');
			return;
		}		
		XmlReg.onreadystatechange = JSfncMyStateChangedForUniUserregistration;
		//alert(colcod);
		XmlReg.open('GET', 'getAjaxdetails.php?listcol='+1+'&collcod='+colcod, true);
		XmlReg.send(null);	
		
		}



