	function valid(object)
{
	if (object.optcname1.value==0) 
		{	
		alert("You must select your programme.");
		object.optcname1.focus();
		return false;
		}
	if (object.sem.value==0) 
		{	
		alert("You must select your semester.");
		object.sem.focus();
		return false;
		}
	if (object.month.value==0) 
		{	
		alert("You must select exam month");
		object.month.focus();
		return false;
		}
	if (object.yr.value==0) 
		{	
		alert("You must select year of exam");
		object.yr.focus();
		return false;
		}		
}
