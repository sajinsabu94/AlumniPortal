function checkEmail(id) {
	if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(id)) {
		return (true);
	}
	return (false);
}

function isValidKey(evt)
{	
	var charCode = (evt.which) ? evt.which : event.keyCode	
	if ((charCode > 31 && (charCode < 48 || charCode > 57)) && charCode != 32)
	
		 	return false;
	return true;
}
function isValidKeyOnlyNumbers(evt)
{	

	var charCode = (evt.which) ? evt.which : event.keyCode		
	if ((charCode < 48 || charCode > 57) && (charCode != 8))
	
		 	return false;
	return true;
}
function isValidAlphabetKey(evt)
{	   
	var charCode = (evt.which) ? evt.which : event.keyCode
	//alert(charCode);
	if ((charCode == 46) || (charCode >= 65 && charCode <=90) || (charCode >= 97 && charCode <=122) || (charCode == 8) || (charCode == 32))
	
		 	return true;
	return false;
}
function isValidAddressHouseAndStreetKey(evt)
{	   
	var charCode = (evt.which) ? evt.which : event.keyCode	
	if ((charCode == 46) || (charCode == 47) ||(charCode == 45) ||(charCode == 44) || (charCode >= 48 && charCode <=57) || (charCode >= 65 && charCode <=90) || (charCode >= 97 && charCode <=122) || (charCode == 8) || (charCode == 32))
	
		 	return true;
	return false;
}
function isValidAddressKey(evt)
{	   
	var charCode = (evt.which) ? evt.which : event.keyCode	
	if ((charCode == 46) || (charCode >= 48 && charCode <=57) || (charCode >= 65 && charCode <=90) || (charCode >= 97 && charCode <=122) || (charCode == 8) || (charCode == 32))
	
		 	return true;
	return false;
}
function isValidAlphabetAndNumbersKey(evt)
{	   
	var charCode = (evt.which) ? evt.which : event.keyCode		
	if ((charCode >= 48 && charCode <=57) || (charCode >= 65 && charCode <=90) || (charCode >= 97 && charCode <=122) || (charCode == 8))
	
		 	return true;
	return false;
}

function isValidKeyWithDot(evt)  // with dot and backspace
{		
	var charCode = (evt.which) ? evt.which : event.keyCode	
	if ((charCode < 48 || charCode > 57) && (charCode != 8) && (charCode != 46))
	
		 	return false;
	return true;
}

